{
  "name": "om",
  "version": "0.1.1",
  "description": "VOICEVOX TTS Bot for Discord.js Japan User Group",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "node --loader ts-node/esm --require dotenv/config src/main.ts",
    "build": "node build.js",
    "start": "node dist/main.js",
    "tsc:check": "tsc --noEmit",
    "lint:check": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "prepare": "simple-git-hooks || [ \"$NODE_ENV\" = \"production\" ]"
  },
  "author": "Discord.js Japan User Group (discordjs-japan.org)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "20.10.4",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "dotenv": "16.3.1",
    "esbuild": "0.19.9",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "simple-git-hooks": "2.9.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@discordjs/opus": "0.9.0",
    "@discordjs/voice": "0.16.1",
    "discord.js": "14.14.1",
    "node-altjtalk-binding": "https://github.com/femshima/node-altjtalk-binding/releases/download/v0.1.4/node.tar.gz",
    "sodium-native": "4.0.4"
  }
}
