{
  "name": "om",
  "version": "0.3.0",
  "description": "VOICEVOX TTS Bot for Discord.js Japan User Group",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "node --loader ts-node/esm --require dotenv/config src/main.ts",
    "build": "node build.js",
    "start": "node dist/main.js",
    "tsc:check": "tsc --noEmit",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "prepare": "node .husky/install.mjs",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "author": "Discord.js Japan User Group (discordjs-japan.org)",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "8.57.0",
    "@types/eslint__js": "8.42.3",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/node": "20.12.12",
    "dotenv": "16.4.5",
    "esbuild": "0.21.3",
    "esbuild-plugin-version-injector": "1.2.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "globals": "15.3.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.4",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "typescript-eslint": "7.10.0",
    "vitest": "1.6.0"
  },
  "dependencies": {
    "@discordjs-japan/om-syrinx": "github:discordjs-japan/om-syrinx#v0.3.0",
    "@discordjs/opus": "github:femshima/opus#v0.9.0",
    "@discordjs/voice": "0.17.0",
    "discord-markdown-parser": "1.1.0",
    "discord.js": "14.15.2",
    "simple-markdown": "0.7.3",
    "sodium-native": "4.1.1"
  }
}
